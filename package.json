{
  "name": "@udacity/ureact-workspace",
  "description": "Workspace and WorkspaceEditor components for rendering and configuring student workspaces",
  "repository": "https://github.com/udacity/ureact-workspace",
  "version": "0.0.0",
  "scripts": {
    "ucrtools:bump": "ucrtools b",
    "ucrtools:changelog": "ucrtools c",
    "test": "gulp test",
    "start": "gulp",
    "release": "gulp release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@udacity/conventional-release-tools/build/commitizen-config"
    },
    "pre-git": {
      "enabled": true,
      "msg-pattern": "^((fixup! |squash! |revert:)?(\\[(?:.*)\\] )?(\\w+)(?:\\(([^\\)\\s]+)\\))+: (.+))(?:\n|$)",
      "allow-untracked-files": true,
      "msg-pattern-error": "UDACITY: Invalid commit message format.\nIt should be \"change_type(scope): subject\"\nPlease check https://github.com/udacity/conventional-release-tools#format"
    }
  },
  "main": "build/index.js",
  "author": "reem@udacity.com",
  "license": "UNLICENSED",
  "files": [
    "build"
  ],
  "dependencies": {
    "@udacity/ureact-styles": "^1.6.0"
  },
  "devDependencies": {
    "@udacity/eslint-config-ureact": "^1.0.10",
    "@udacity/ureact-package": "^3.1.0",
    "@udacity/web-terminal-client": "^1.1.1",
    "babel-eslint": "^7.1.1",
    "es6-error": "^4.0.2",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^6.7.1",
    "gulp": "^3.9.1",
    "react": ">=0.14.0",
    "react-addons-test-utils": ">=0.14.0",
    "react-dom": ">=0.14.0",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-create-reducer": "^1.1.1",
    "redux-logger": "^3.0.0",
    "redux-saga": "^0.14.3"
  }
}
